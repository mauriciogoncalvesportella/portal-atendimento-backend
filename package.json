{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist && cp ca-certificate.crt dist/",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "install:prod": "yarn install --production --modules-folder ./dist/node_modules",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "typeorm:migrate": "pnpm typeorm migration:generate -- -n",
    "typeorm:run": "pnpm run typeorm migration:run"
  },
  "dependencies": {
    "@nestjs-query/query-typeorm": "^0.19.4",
    "@nestjs/common": "^7.6.18",
    "@nestjs/config": "^0.4.2",
    "@nestjs/core": "^7.6.18",
    "@nestjs/jwt": "^6.1.2",
    "@nestjs/passport": "^6.2.0",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/platform-socket.io": "^7.6.18",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.6.18",
    "@types/bcrypt": "^3.0.1",
    "@types/cors": "^2.8.13",
    "@types/ioredis": "^4.28.10",
    "@types/passport": "^1.0.11",
    "@yarnpkg/pnpify": "^2.4.0",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "cache-manager": "^3.6.3",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.11.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "dotenv": "^16.0.3",
    "global": "^4.4.0",
    "ioredis": "^4.28.5",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.40",
    "multer": "1.4.5-lts.1",
    "nestjs-typeorm-paginate": "^2.6.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.14.0",
    "pg-query-stream": "^4.2.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "sharp": "^0.31.3",
    "shortid": "^2.2.16",
    "typeorm": "^0.2.45",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^6.9.4",
    "@nestjs/testing": "^6.11.11",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cache-manager": "^3.4.3",
    "@types/express": "^4.17.15",
    "@types/jest": "^24.9.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.191",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/sharp": "^0.31.1",
    "@types/socket.io": "^2.1.13",
    "@types/supertest": "^2.0.12",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.14.1",
    "tslint": "^5.20.1",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
